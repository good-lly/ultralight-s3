{
  "name": "ultralight-s3",
  "version": "0.0.1",
  "description": "A ~15KB lightweight S3 client that doesn't require anything and anyone.",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run build:node",
    "build": "tsc",
    "build:node": "tsc --module node16 --outDir ./lib/ &&  esbuild lib/index.js --bundle --outfile=lib/index.min.js --platform=node --format=esm --minify --sourcemap",
    "dev:wrangler": "wrangler dev --port 8787",
    "dev:node": "esbuild src/index.js --bundle --outfile=lib/index.js --platform=node --format=esm --watch=forever & nodemon --w './src/*.js' --exec 'node  --max-old-space-size=4096 --env-file ./.env ./dev/node-dev.js'",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config node.jest.config.json --no-cache",
    "clean": "rm -rf lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sentienhq/femtoS3.git"
  },
  "jest": {
    "projects": [
      "tests/node.jest.config.js"
    ]
  },
  "keywords": [],
  "author": "Peter Jensen @ SentienHQ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sentienhq/femtoS3.js/issues"
  },
  "homepage": "https://github.com/sentienhq/femtoS3.js#readme",
  "devDependencies": {
    "@hono/node-server": "^1.11.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "esbuild": "^0.21.5",
    "hono": "^4.4.5",
    "jest": "^29.7.0",
    "minio": "^8.0.1",
    "nodemon": "^3.1.3",
    "typescript": "^5.5.2"
  }
}
